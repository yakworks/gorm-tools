{
  "name": "9ci-openapi-spec",
  "version": "0.0.2",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.1",
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@apidevtools/swagger-parser": "^10.0.2",
    "@redocly/openapi-cli": "1.0.0-beta.49",
    "@rollup/plugin-replace": "^2.4.2",
    "autoprefixer": "^10.2.6",
    "cheerio": "^1.0.0-rc.9",
    "cross-var": "^1.1.0",
    "crypto-js": "^4.0.0",
    "cssnano": "^5.0.6",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "eleventy-plugin-toc": "^1.1.0",
    "fast-safe-stringify": "^2.0.7",
    "html-minifier": "^4.0.0",
    "imagesloaded": "^4.1.4",
    "jquery": "^3.6.0",
    "jsdom": "^16.6.0",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "markdown-it": "^12.0.6",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-prism": "^2.1.6",
    "openapi-sampler": "^1.0.1",
    "postcss": "^8.3.2",
    "postcss-cli": "^8.3.1",
    "reftools": "^1.1.8",
    "rollup": "^2.51.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.34.0",
    "sharp": "^0.30.5",
    "shelljs": "^0.8.4",
    "tailwindcss": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-inclusive-language": "^2.1.1",
    "husky": "^6.0.0",
    "markdownlint": "^0.23.1",
    "markdownlint-cli2": "^0.1.3",
    "prettier": "^2.3.1"
  },
  "private": true,
  "scripts": {
    "oapi:bundle": "openapi bundle -o build/api",
    "oapi:lint": "openapi lint build/api.yaml",
    "lint": "npm run lint:markdown && npm run lint:js",
    "lint:markdown": "npx markdownlint-cli2 \"./content/**/*.md\"",
    "lint:js": "eslint '**/*.js'",
    "start": "npm run oapi:bundle && npm run oapi:lint && npm run serve",
    "build.sass": "npx sass --update --style compressed --no-source-map ./eleventy/slate/css:./eleventy/slate/css",
    "serve": "npm run build.sass && npx eleventy --serve --port=4567",
    "build": "npm run build:oapi && npm run build.sass && npm run build:eleventy ",
    "build:oapi": "npm run oapi:bundle && npm run oapi:lint",
    "build:eleventy": "eleventy"
  }
}
